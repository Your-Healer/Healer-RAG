# OpenAI API settings
OPENAI_API_KEY=your_openai_api_key

# Vector database settings
# Choose your vector store by setting DEFAULT_VECTOR_STORE to "chroma" or "pgvector"
DEFAULT_VECTOR_STORE=chroma

# Chroma settings (local vector database)
CHROMA_PERSIST_DIRECTORY=./chroma_langchain_db

# PostgreSQL with pgvector settings
PGVECTOR_HOST=localhost
PGVECTOR_PORT=5432
PGVECTOR_DATABASE=healer_vector_db
PGVECTOR_USER=postgres
PGVECTOR_PASSWORD=your_password_here
# Use connection_url format for PGVector:
# PGVECTOR_CONNECTION_STRING=postgresql://user:password@localhost:5432/healer_vector_db
